        <div id="Login">
            <div id="Login_icon">
                <img src="https://flaticons.net/icon.php?slug_category=application&slug_icon=user-login" alt="Login Icon">
            </div>
            <p>Login</p>

        </div>
        <div id="Register">
            <div id="Register_logo">
                <img src="https://p.kindpng.com/picc/s/83-831275_fc-barcelona-white-logo-png-transparent-png.png" alt="Register Logo">
            </div>
            <p>Register</p>
        </div>
        <div id="Language">
            <p>EN</p>
        </div>
    </div>
    <div class="header_midd">
        <div class="Logo">
            <a href="/">
            <img
                src="https://assets.stickpng.com/images/584a9b3bb080d7616d298777.png"
                alt="FCB Logo"
                
            />
            </a>
        </div>
        <div class="Left_nav">
            <div class="SubMenu" id="FirstTeam">
                <p>FIRST TEAM &#9662;</p>
            </div>
